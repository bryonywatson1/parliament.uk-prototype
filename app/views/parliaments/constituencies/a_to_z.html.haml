.section--primary
  .container
    %h1
      %span= t('constituency_plural').capitalize
      = link_to "#{@parliament.date_range(date_format: '%Y')} #{t('parliament')} #{t('parliaments.a_to_z')}", parliament_path(@parliament.graph_id)
      %span= "A-Z"

    = render partial: 'pugin/components/navigation-letter', locals: { route_symbol: :parliament_constituencies_a_z_letter_path, route_args: [@parliament.graph_id] }
